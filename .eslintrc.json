{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "ignoreRestSiblings": true
      }
    ],
    "react/jsx-no-useless-fragment": "warn",
    "no-unused-expressions": "warn"
  }
}
